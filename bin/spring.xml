<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">
<beans>
	<bean id="triangle" class="com.myseries.spring.Triangle">
		<property name="points">
			<list>
				<ref bean="point1" />
				<ref bean="point2" />
				<ref bean="point3" />
			</list>
		</property>
	</bean>
	
	<bean id="point1" class="com.myseries.spring.Point">
		<property name="x" value="20" />
		<property name="y" value="20" />
	</bean>
	
	<bean id="point2" class="com.myseries.spring.Point">
		<property name="x" value="0" />
		<property name="y" value="40" />
	</bean>
	
	<bean id="point3" class="com.myseries.spring.Point">
		<property name="x" value="40" />
		<property name="y" value="40" />
	</bean>
	
</beans>	   		